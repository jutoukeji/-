/* pages/main/main.wxss */
.header{
  height: 120rpx;
  background: linear-gradient(#444, #555);
  color: #FFFFFF;
  padding: 16rpx;
  font-size: 26rpx;
}
.image-box{
  margin-right: 16rpx;
  width: 140rpx;
  height: 120rpx;
  border-radius: 6rpx;
}
.body {
  background: #FFF;
  height: 100%;
}
	
.menu {
  background: #efefef;
  font-size: 24rpx;
  color: #666;
  padding-bottom: 30rpx;
  position: relative;
}

.menu .ul {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 128rpx;
  overflow-y: auto;
}

.menu .ul .li {
  padding: 24rpx 16rpx;
  position: relative;
}

.menu .ul .li .mark{
  content: '';
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  height: 24rpx;
  width: 24rpx;
  border-radius: 50%;
  background: #ff5200;
  color: #fff;
  text-align: center;
  line-height: 24rpx;
  font-size: 20rpx;
}

.menu .ul .li.active {
  color: #000;
  background: #FFF;
  font-weight: bold;
}

.pdt-list {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 108rpx;
  top: 0;
  transition: all 0.5s;
  scroll-behavior: smooth;
}

.nav-title {
  line-height: 72rpx;
  margin-left: 20rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  background: #FFF;
}
.footer {
		position: fixed;
		left: 0;
		right: 0;
		bottom: 0;
		height: 108rpx;
		background: #555;
		color: #FFF;
		z-index: 99;
	}
	
.cycle {
  position: absolute;
  top: -20rpx;
  bottom: 20rpx;
  left: 50%;
  width: 88rpx;
  height: 88rpx;
  margin-left: -54rpx;
  border: 10rpx solid #505050;
  border-radius: 50%;
  background: #ff802b url('data:image/png;base64,') center no-repeat;
  background-size: 64rpx;
}

.cycle.disabled {
  border: 10rpx solid #666;
  background: #444 url('data:image/png;base64,') center no-repeat;
  background-size: 64rpx;
}

.cycle.disabled .i {
  display: none;
}

.cycle .i {
  content: "";
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  width: 32rpx;
  height: 32rpx;
  font-size: 24rpx;
  text-align: center;
  background: #ff5200;
  border-radius: 50%;
}

.shopping-cart {
  height: 100%;
}

.shopping-cart .price .p {
  font-size: 36rpx;
  line-height: 104rpx;
}

.shopping-cart .price .span {
  font-size: 24rpx;
  color: #999;
}

.submit-btn {
  width: 100%;
  display: inline-block;
  line-height: 108rpx;
  text-align: center;
  color: #FFF;
  font-size: 32rpx;
  font-weight: bold;
  background: #ff8800;
}

.submit-btn.disabled {
  background: #666;
  color: #999;
}

.cart-list .cart-container {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 108rpx;
  z-index: 98;
  background: #FFF;
  color: #666;
}

.cart-list .ul {
  max-height: 600rpx;
}

.cart-list .ul .li {
  padding: 30rpx 20rpx;
}

.cart-list .cover {
  content: '';
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 108rpx;
  background: rgba(0, 0, 0, 0.5);
  z-index: 97;
}

.cart-title {
  padding: 0 30rpx 0 0 !important;
  text-align: right;
  line-height: 72rpx;
  color: #999;
  font-size: 24rpx;
  background: #eaeaea;
}

.cart-title .span {
  padding-left: 36rpx;
  position: relative;
  line-height: 32rpx;
  display:inline-block;
}

.cart-title .span::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 32rpx;
  height: 32rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: 24rpx;
}

.overflow-hide {
  overflow-y: hidden;
}



.sus-box{
  position: fixed;
  left: 100rpx;
  bottom: 200rpx;
  z-index: 103;
  cursor: pointer;
  background: rgba(0,0,0, 0.7);
  padding: 16rpx 20rpx;
  border-radius: 34rpx;
}
.sus-box .a{
  color: #FFF;
  font-size: 24rpx;
}
.category-title {
  line-height: 72rpx;
  margin-left: 20rpx;
}

.item-box {
  padding: 20rpx;
}

.img-box {
  height: 140rpx;
  width: 160rpx;
}

.img-box img {
  height: 100%;
}

.def-img {
  width: 100%;
  height: 100%;
  background: #ddd;
}

.content {
  position: relative;
  margin-left: 20rpx;
}

.content .name {
  font-size: 24rpx;
  font-weight: bold;
  color: #000;
}
.content .small{
  font-size: 20rpx;
}
.content .small .em:first-child {
  display: inline-block;
  width: 160rpx;
}

.bottom {
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
}

.pirce {
  color: red;
  font-weight: bold;
  line-height: 48rpx;
}

.opt {
  color: #444;
}

.opt .add {
  width: 48rpx;
  height: 48rpx;
  background: #f8a167 url('data:image/png;base64,') center no-repeat;
  background-size: 32rpx;
  vertical-align: middle;
  border-radius: 50%;
}

.opt .reduce {
  width: 44rpx;
  height: 44rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgCAYAAABzenr0AAAASUlEQVRYR+3VwQ0AIAhDURitk3c0XUA9kdTDZwBKHiF0havD+cUACCCAAAIIXAVsr8lHJemYxQD/Ckzu/9WLM0QAAQQQQCAusAGM3RAhvZBmdQAAAABJRU5ErkJggg==') center no-repeat;
  background-size: 32rpx;
  vertical-align: middle;
  border: 2rpx solid #bfbfbf;
  border-radius: 50%;
}
	
.opt .span {
  width: 40rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
}


.fly-item,
.fly-item image {
  position: fixed;
  transition: transform .4s;
  width: 48rpx;
  height: 48rpx;
  z-index: 101;
  border-radius: 50%;
}

.fly-item {
  display: none;
  margin: -24rpx 0 0 -24rpx;
  transition-timing-function: linear;
  opacity: 0.5;
}

.fly-item image {
  transition-timing-function: cubic-bezier(.55, 0, .85, .36);
}
 
.detail-container{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 108rpx;
  right: 0;
  z-index: 98;
  background: #FFF;
}
.detail-container image{
  width: 100%;
  height: 60%;
}
.detail-container .content-box{
  padding: 20rpx;
}
.content-box .title{
  font-size: 48rpx;
  font-weight: bold;
}
.content-box .sub-title{
  font-size: 24rpx;
  color: #666;
  line-height: 60rpx;
}
.content-box .price{
  line-height:48rpx;
  font-size:36rpx;
  color:#ff2200;
}
.mright40{
  margin-right: 80rpx;
}
.detail-container .icon-close{
  position: absolute;
  top: 0;
  right: 0;
  height: 100rpx;
  width: 100rpx;
  
}

.detail-container .icon-close::before{
  content: '';
  position: absolute;
  width: 64rpx;
  height: 64rpx;
  background: rgba(158, 158, 158, 0.5);
  border-radius: 50%;
  top: 18rpx;
  left: 18rpx;
}
.detail-container .icon-close::after{
  content: '';
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  background: url('data:image/png;base64,')center no-repeat;
  background-size: 52rpx;
}

.float-container{
  position: fixed;
  left: 160rpx;
  bottom: 200rpx;
  border-radius: 80rpx;
  background: rgba(0, 0, 0, 0.7);
  color: #ddd;
  line-height: 80rpx;
}
.float-container text,
.float-container navigator{
  font-size: 24rpx;
  overflow:hidden;
  text-overflow:clip;
  white-space:nowrap;
  text-align: center;
}
.float-container .menu-name{
  width: 80rpx;
  height: 80rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgCAYAAABzenr0AAAAVUlEQVRYR+2Uyw0AIAhD272cwV0dwrlwAg0JBzw8znxfWqzmcPN8sQAEIPAkEBFL0ixaddsetx5/L1C8PFWOCCHwtwva/0DKR8UkRAgBbIgGIAABCBxP8x4h25xtjwAAAABJRU5ErkJggg==')center no-repeat;
  background-size: 48rpx;
}
.float-container .menu-list{
  transition: width .3s;
}