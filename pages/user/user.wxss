page{
  background: #ececec;
}
.container{
  height: auto;
}
.userinfo{
  background: linear-gradient(#444, #555);
  padding: 30rpx;
  height: 160rpx;
  color: #ddd;
}
.userinfo-avatar{
  height: 130rpx;
  width: 130rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.userinfo-nickname{
  color: white;
  font-size: 32rpx;
  line-height: 50rpx;
}
.menu-box{
  margin: 20rpx 0;
  background: white;
}
.menu-box navigator{
  line-height: 88rpx;
}
.menu-box .btn{
  text-align: center;
  font-size: 32rpx;
  background: white;
}
.menu-item{
  padding-left: 58rpx;
  border-bottom: 1rpx solid #ececec;
  position: relative;
}
.menu-item::after{
  content: '';
  position: absolute;
  top: 0;
  right: 20rpx;
  height: 100%;
  width: 32rpx;
  background: url('data:image/png;base64,')center no-repeat;
  background-size: 32rpx;
}
.menu-item.order{
  background: url('data:image/png;base64,')20rpx center no-repeat;
  background-size: 28rpx;
}
.menu-item.discount{
  background: url('data:image/png;base64,')20rpx center no-repeat;
  background-size: 28rpx;
}

